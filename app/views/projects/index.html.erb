<p id="notice"><%= notice %></p>

<!--Centered Image Start-->

<div style="text-align: center;">
  <img width="400" src="https://www.forevergreen.tech/wp-content/uploads/elementor/thumbs/IMG_1671-psz3x93oamj6zsrhk8y60r5u6wlmgbczanolsql1c0.png">
</div>

<!--Centered Image End-->

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.description %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

